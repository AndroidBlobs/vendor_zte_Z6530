# MTK project .rc configure

on post-fs-data
#liuliang, for ztee
    #chmod 0666 /dev/tee0
    #chown system system /dev/tee0
    #chmod 0666 /dev/teepriv0
    #chown system system /dev/teepriv0

    mkdir /data/vendor/tee 0660 root root
    mkdir /mnt/vendor/persist/data  0760 system system
    mkdir /mnt/vendor/persist/data/gatekeeper  0760 system system
#liuliang end

on init
#liuliang, for ztee
    chmod 0666 /dev/tee0
    chown system system /dev/tee0
    chmod 0666 /dev/teepriv0
    chown system system /dev/teepriv0
#liuliang end

#liuliang, for ztee
service tee-supplicant /vendor/bin/tee-supplicant
    class core
    user root
    group root
    seclabel u:r:tee:s0
#liuliang